                  </m:mrow>

                  <m:mi>n</m:mi>
                </m:munderover>

                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>

                    <m:mrow>
                      <m:mrow>
                        <m:mo>(</m:mo>

                        <m:mrow>
                          <m:msub>
                            <m:mi>p</m:mi>

                            <m:mi>i</m:mi>
                          </m:msub>

                          <m:mo>-</m:mo>

                          <m:mover>
                            <m:mi>o</m:mi>

                            <m:mo>─</m:mo>
                          </m:mover>
                        </m:mrow>

                        <m:mo>)</m:mo>
                      </m:mrow>

                      <m:mo>+</m:mo>

                      <m:mrow>
                        <m:mo>(</m:mo>

                        <m:mrow>
                          <m:msub>
                            <m:mi>o</m:mi>

                            <m:mi>i</m:mi>
                          </m:msub>

                          <m:mo>-</m:mo>

