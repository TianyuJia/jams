          <m:mo>=</m:mo>

          <m:mrow>
            <m:mn>1</m:mn>

            <m:mo>-</m:mo>

            <m:mfrac>
              <m:mrow>
                <m:munderover>
                  <m:mo>∑</m:mo>

                  <m:mrow>
                    <m:mi>i</m:mi>

                    <m:mo>=</m:mo>

                    <m:mn>1</m:mn>
                  </m:mrow>

                  <m:mi>n</m:mi>
                </m:munderover>

                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>

                    <m:mrow>
                      <m:msub>
                        <m:mi>p</m:mi>

                        <m:mi>i</m:mi>
                      </m:msub>

                      <m:mo>-</m:mo>

                      <m:msub>
                        <m:mi>o</m:mi>

                        <m:mi>i</m:mi>
                      </m:msub>
